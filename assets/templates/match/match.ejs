<div id="match">
	<div class="row">
		<div class="col-md-9 text-center">
			<div class="row">
				<div class="col-md-2 col-md-offset-3">
					<h2><strong><%= lightPlayer.username%></strong></h2>
				</div>
				<div class="col-md-2">
					<h2>vs</h2>
				</div>
				<div class="col-md-2">
					<h2><strong><%= darkPlayer.username%></strong></h2>
				</div>
			</div>

			<div class="row">
				<div class="col-md-12 text-center" id="state"/>
			</div>
			<div id="board-container"/>
		</div>
		<div class="col-md-3 right-side-bar well well-lg">
			<strong>Chat</strong>
			<div class="chat panel panel-default">
			</div>
			<input type="text" class="form-control" placeholder="Type here...">
		</div>
	</div>
</div>